{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE"
      ],
      "has": [
        {
          "type": "header",
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "type": "header",
          "key": "Access-Control-Allow-Origin",
          "value": "https://gs-backoffice-frontend.vercel.app"
        }
      ]
    }
  ]
}